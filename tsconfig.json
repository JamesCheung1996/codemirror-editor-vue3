{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "esnext",
    "lib": ["DOM", "ESNext"],
    "types": ["vite/client"],
    "jsx": "preserve",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "incremental": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["packages/*"],
      "./*": ["./*"]
    }
  },
  "include": [
    "packages/**/*",
    // "packages",
    // "packages/*",
    // "playground/**/*",
    // "playground",
    "types",
    "*.ts",
    "*.tsx",
    "*.vue",
    "*.d.ts"
  ]
  // "exclude": ["build/**/*"]
}
